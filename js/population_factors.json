{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "Stacked bar chart of population factors by Australian states",
    "data": {
      "url": "", 
      "format": {
        "type": "csv"
      }
    },
    "transform": [
      {
        "fold": ["NSW", "Vic.", "Qld", "SA", "WA", "Tas.", "NT", "ACT"],
        "as": ["State", "Value"]
      }
    ],
    "mark": "bar",
    "encoding": {
      "x": {
        "field": "State",
        "type": "nominal",
        "title": "State"
      },
      "y": {
        "field": "Value",
        "type": "quantitative",
        "title": "Population Factor"
      },
      "color": {
        "field": "Metric",
        "type": "nominal",
        "title": "Metric"
      }
    }
  }
  